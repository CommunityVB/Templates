{
  "$schema": "http://json.schemastore.org/template",
  "author": "CommunityVB Contributors",
  "classifications": [
    "Common",
    "Console"
  ],
  "name": "Console (Async)",
  "identity": "CommunityVB.Console.Async",
  "groupIdentity": "CommunityVB.Console.Async",
  "shortName": "console.async.vb",
  "tags": {
    "language": "VB",
    "type": "project"
  },
  "sourceName": "console.async",
  "preferNameDirectory": true,
  "symbols": {
    "targetframework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "choices": [
        {
          "choice": "net6.0",
          "description": "Target net6.0"
        },
        {
          "choice": "net7.0",
          "description": "Target net7.0"
        },
        {
          "choice": "net8.0",
          "description": "Target net8.0"
        }
      ],
      "defaultValue": "net8.0",
      "replaces": "$(FrameworkParameter)"
    }
  },
  "primaryOutputs": [
    { "path": "console.async.vbproj" }
  ]
}