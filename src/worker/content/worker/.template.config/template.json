{
  "$schema": "http://json.schemastore.org/template",
  "author": "CommunityVB Contributors",
  "classifications": [
    "Common",
    "Worker",
    "Web"
  ],
  "name": "Worker Service",
  "identity": "CommunityVB.Worker.Empty",
  "groupIdentity": "CommunityVB",
  "shortName": "worker.vb",
  "tags": {
    "language": "VB",
    "type": "project"
  },
  "sourceName": "WorkerService",
  "preferNameDirectory": true,
  "symbols": {
    "targetframework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "choices": [
        {
          "choice": "net6.0",
          "description": "Target net6.0"
        },
        {
          "choice": "net7.0",
          "description": "Target net7.0"
        },
        {
          "choice": "net8.0",
          "description": "Target net8.0"
        }
      ],
      "defaultValue": "net8.0",
      "replaces": "$(FrameworkParameter)"
    }
  },
  "primaryOutputs": [
    { "path": "WorkerService.vbproj" }
  ]
}